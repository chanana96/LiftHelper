<% if (!newProfile.friendRequests[0]) {%>
<h3>You have no pending friend requests right now</h3>
<%}%>
<%- include('partials/messages'); %>
<% newProfile.friendRequests.forEach(fr => { %>
	<p><a href="/users/profile/<%= fr.requestUsername %>"><%= fr.requestUsername %></a></p>
	<form action="/users/friendrequests/<%= fr.requestUsername %>" method="POST" class="d-inline">
		<button type="submit" class="btn btn-danger" style="float: right;">Accept friend</button>
	</form>
<% }) %>